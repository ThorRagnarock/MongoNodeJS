#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/amosvatury/Desktop/פרוייקט גמר - ניירות ומקורות השראה/_rclstServer/NodeJS_2/PnpmExpress/client/node_modules/.pnpm/browserslist@4.21.10/node_modules/browserslist/node_modules:/Users/amosvatury/Desktop/פרוייקט גמר - ניירות ומקורות השראה/_rclstServer/NodeJS_2/PnpmExpress/client/node_modules/.pnpm/browserslist@4.21.10/node_modules:/Users/amosvatury/Desktop/פרוייקט גמר - ניירות ומקורות השראה/_rclstServer/NodeJS_2/PnpmExpress/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/amosvatury/Desktop/פרוייקט גמר - ניירות ומקורות השראה/_rclstServer/NodeJS_2/PnpmExpress/client/node_modules/.pnpm/browserslist@4.21.10/node_modules/browserslist/node_modules:/Users/amosvatury/Desktop/פרוייקט גמר - ניירות ומקורות השראה/_rclstServer/NodeJS_2/PnpmExpress/client/node_modules/.pnpm/browserslist@4.21.10/node_modules:/Users/amosvatury/Desktop/פרוייקט גמר - ניירות ומקורות השראה/_rclstServer/NodeJS_2/PnpmExpress/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
